{"ast":null,"code":"import { ref } from 'vue';\nimport axios from 'axios';\nexport default {\n  setup() {\n    const groups = ref([]);\n    const fetchGroups = async () => {\n      try {\n        const response = await axios.get('http://localhost:4000/api/grupos');\n        groups.value = response.data;\n      } catch (error) {\n        console.error('Error fetching groups:', error);\n      }\n    };\n    return {\n      groups,\n      fetchGroups\n    };\n  }\n};","map":{"version":3,"names":["ref","axios","setup","groups","fetchGroups","response","get","value","data","error","console"],"sources":["/home/oussama/Desktop/HackatonF5-oussama/Frontend/hackathon-admin/src/components/TeamDetail.vue"],"sourcesContent":["<template>\n  <div class=\"containerGral\">\n    <button class=\"button-view\" @click=\"fetchGroups\">{{ $t('ShowGroups') }}</button>\n    <div v-if=\"groups.length\">\n      <div class=\"container\">\n        <div v-for=\"(group, groupIndex) in groups\" :key=\"groupIndex\" class=\"card-grid\">\n          <h4>{{ $t('Group') }} {{ groupIndex + 1 }}</h4>\n          <div class=\"card\" v-for=\"(person, personIndex) in group\" :key=\"personIndex\">\n            <div class=\"grid-container\">\n              <div class=\"grid-item\"><p><b><em>{{ $t('groups.name') }}:</em></b></p> {{ person.nombre }}</div>\n              <div class=\"grid-item\"><p><b><em>{{ $t('groups.email') }}: </em></b></p>{{ person.email }}</div>\n              <div class=\"grid-item\"><p><b><em>{{ $t('groups.front') }}: </em></b></p>{{ person.front }}</div>\n              <div class=\"grid-item\"><p><b><em>{{ $t('groups.back') }}:</em></b></p> {{ person.back }}</div>\n              <div class=\"grid-item\"><p><b><em>{{ $t('groups.bootcamp') }}:</em> </b></p>{{ person.bootcamp }}</div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from 'vue';\nimport axios from 'axios';\n\nexport default {\n  setup() {\n    const groups = ref([]);\n\n    const fetchGroups = async () => {\n      try {\n        const response = await axios.get('http://localhost:4000/api/grupos');\n        groups.value = response.data;\n      } catch (error) {\n        console.error('Error fetching groups:', error);\n      }\n    };\n\n    return {\n      groups,\n      fetchGroups\n    };\n  }\n};\n</script>\n\n  \n<style scoped>\n.container {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  padding-top: 2rem;\n  width: 100%;\n  overflow-x: auto; /* Enable horizontal scrolling */\n}\n\n.card-grid {\n  width: 45%; /* Adjust the width to fit within the container */\n  margin-bottom: 2rem;\n  padding: 1rem;\n  border: 1px solid #ccc;\n  border-radius: 8px;\n  box-sizing: border-box; /* Ensure padding is included in the width */\n  overflow-x: auto; /* Enable horizontal scrolling for individual groups */\n}\n\n.card {\n  margin-bottom: 1rem;\n}\n\n.grid-container {\n  display: grid;\n  grid-template-columns: repeat(6, 1fr);\n  gap: 1rem;\n}\n\n.button-view {\n  background-color: #09ea51; \n  border: none;\n  color: white;\n  padding: 0.625rem 1.25rem;\n  font-size: 1rem;\n  border-radius: 0.25rem;\n  cursor: pointer;\n}\n\n</style>\n"],"mappings":"AAuBA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIH,GAAG,CAAC,EAAE,CAAC;IAEtB,MAAMI,WAAU,GAAI,MAAAA,CAAA,KAAY;MAC9B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMJ,KAAK,CAACK,GAAG,CAAC,kCAAkC,CAAC;QACpEH,MAAM,CAACI,KAAI,GAAIF,QAAQ,CAACG,IAAI;MAC9B,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;MAChD;IACF,CAAC;IAED,OAAO;MACLN,MAAM;MACNC;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}